<div class ="home white row">
  <div aria-label="Competition Photos" class="orbit image-orbit" data-orbit="" role="region"><button class="orbit-previous"><span class="show-for-sr">Previous Slide</span>&#9664;&#xFE0E;</button>
    <button class="orbit-next"><span class="show-for-sr">Next Slide</span>&#9654;&#xFE0E;</button>
    <ul class="orbit-container">
      <%= @images.each do |image| %>
        <li class="is-active orbit-slide">
          <img alt="photo-<%= image.caption %>" class="orbit-image" src="<%= image.image_location(:orbit) %>" />
          <% unless image.caption.empty? %>
            <figcaption class="orbit-caption"><%= image.caption %></figcaption>
          <% end %>
        </li>
      <% end %>%>
    </ul>
  </div>
</div>
